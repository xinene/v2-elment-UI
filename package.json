{
  "name": "v2-elment-ui",
  "description": "element-ui for vue2",
  "auth": "kele_hx",
  "version": "1.0.0",
  "main": "lib/v2-elment-ui.umd.js",
  "homepage": "https://github.com/xinene/v2-elment-UI.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/xinene/v2-elment-UI.git"
  },
  "private": false,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "yarn && vue-cli-service build",
